                /////////////////////
                var dataKendo = [
                    {
                        type: 'Company',
                        category: "Overall",
                        from: @Model.OrganizationScoreTotal,
                to: @Model.OrganizationScoreTotal+1,
                    color: "black"
                        }, {
                    type: '5,700 Firms',
                        category: "Overall",
